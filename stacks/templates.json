{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 2,
      "title": "Documentation Stack (Wiki.js + Draw.io)",
      "description": "Central documentation portal with Wiki.js, PostgreSQL, and Draw.io",
      "categories": [ "Documentation" ],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/wikijs.png",
      "note": "Deploys Wiki.js + PostgreSQL + Draw.io. Traefik labels are commented out, enable them later.",
      "name": "documentation-stack",
      "repository": {
        "url": "https://github.com/shackofnoreturn/shackhq-docker-swarm",
        "stackfile": "stacks/dev/docs/docker-compose.docs.yml"
      },
      "env": [
        {
          "name": "STACK_NAME",
          "label": "Stack name",
          "default": "Documentation Stack"
        },
        {
          "name": "WIKI_DOMAIN",
          "label": "Wiki.js domain",
          "default": "wiki.example.com"
        },
        {
          "name": "DRAWIO_DOMAIN",
          "label": "Draw.io domain",
          "default": "draw.example.com"
        },
        {
          "name": "DB_PASS",
          "label": "Postgres password",
          "default": "wikijs_pass"
        },
        {
          "name": "TRAEFIK_RESOLVER",
          "label": "Traefik certresolver name",
          "default": "myresolver"
        }
      ]
    }
  ]
}